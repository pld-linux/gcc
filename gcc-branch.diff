+                    Delt_Time : out Duration) is
+      Year         : Year_Number;
+      Month        : Month_Number;
+      Day          : Day_Number;
+      Minute       : Integer := 0;
+      Idx          : Integer := Str'First;
+      Ch           : Character := Str (Idx);
