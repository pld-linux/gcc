+         Idx := Idx + 1;
+         Ch  := Str (Idx);
+      end loop;
+      if Time_Type = Absolute_Time then
+         Abs_Time := Time_Of (Year, Month, Day, Day_Duration (1));
+      else
+         Delt_Time := Duration (Float (Minute));
+      end if;
